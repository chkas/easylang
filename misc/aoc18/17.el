# AoC-18 - Day 17: Reservoir Research
# 
visual = 1
# 
global f[] maxpos minpos .
# 
func read . .
  len f[] 300 * 2500
  miny = 9999
  maxy = 0
  repeat
    s$ = input
    until s$ = ""
    s$[] = strsplit s$ "., "
    a = number substr s$[0] 2 9
    b = number substr s$[2] 2 9
    c = number s$[4]
    if substr s$[0] 0 1 = "x"
      for y = b to c
        p = y * 300 + a - 300
        f[p] = 1
      .
    else
      for x = b to c
        p = a * 300 + x - 300
        f[p] = 1
      .
      b = a ; c = a
    .
    miny = lower miny b
    maxy = higher maxy b
  .
  minpos = miny * 300
  maxpos = maxy * 300 + 300
.
call read
# 
background 765
global pos0 skip .
func show p . .
  if visual = 0
    break 1
  .
  skip = (skip + 1) mod 10
  if skip < 6
    break 1
  .
  p = p div 300 * 300 - 30000
  if p > pos0 + 60000
    pos0 = p
  .
  clear
  sz = 1 / 3
  for y range 300
    x = 0
    while x < 300
      h = f[x + 300 * y + pos0]
      x += 1
      if h > 0
        x0 = x - 1
        while f[x + 300 * y + pos0] = h
          x += 1
        .
        if h = 1
          color 000
        elif h = 2
          color 066
        elif h = 3
          color 008
        .
        move x0 * sz y * sz
        rect sz * (x - x0) sz
      .
    .
  .
  sleep 0
.
func run . .
  pos = 500 - 300
  while pos < minpos
    pos += 300
  .
  todo_n[] &= pos
  while len todo_n[] > 0
    len todo[] 0
    swap todo[] todo_n[]
    for i_todo range len todo[]
      pos = todo[i_todo]
      while pos < maxpos and f[pos] = 0
        f[pos] = 3
        pos += 300
      .
      call show pos
      if f[pos] <> 3 and pos < maxpos
        repeat
          l = pos - 300
          while f[l] <> 1 and f[l + 300] <> 0
            f[l] = 3
            l -= 1
          .
          r = pos - 300
          while f[r] <> 1 and f[r + 300] <> 0
            f[r] = 3
            r += 1
          .
          until f[l] <> 1 or f[r] <> 1
          for h = l + 1 to r - 1
            f[h] = 2
          .
          call show pos
          pos -= 300
        .
        if f[l] = 0
          todo_n[] &= l
        .
        if f[r] = 0
          todo_n[] &= r
        .
      .
    .
  .
  call show pos
  for i range len f[]
    if f[i] > 1
      sum += 1
      if f[i] = 2
        sum2 += 1
      .
    .
  .
  print sum
  print sum2
.
call run
