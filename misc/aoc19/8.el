# AoC-19 - Day 8: Space Image Format
# 
img[] = number strchars input
w = 25
h = 6
sz = w * h
n_layer = len img[] / sz
# 
func part1 . .
  min = 1 / 0
  for l range n_layer
    cnt = 0
    for i range sz
      ind = sz * l + i
      if img[ind] = 0
        cnt += 1
      .
    .
    if cnt < min
      min = cnt
      min_l = l
    .
  .
  for i range sz
    ind = sz * min_l + i
    if img[ind] = 1
      cnt1 += 1
    elif img[ind] = 2
      cnt2 += 1
    .
  .
  print cnt1 * cnt2
.
call part1
# 
func part2 . .
  len img2[] sz
  for i range sz
    for l = n_layer - 1 downto 0
      ind = sz * l + i
      if img[ind] < 2
        img2[i] = img[ind]
      .
    .
  .
  background 000
  color 777
  clear
  for i range sz
    if img2[i] = 1
      x = i mod w * 3 + 10
      y = i div w * 3 + 10
      move x + 1.5 y + 1.5
      circle 1.8
    .
  .
.
call part2
