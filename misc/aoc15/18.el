# AoC-15 - Day 18: Like a GIF For Your Yard
# 
visualization = 1
# 
repeat
  s$ = input
  until s$ = ""
  inp$[] &= s$
.
n = len inp$[] + 1
# 
f = 100 / (n - 1)
background 000
color 543
subr show
  if visualization = 1
    clear
    for r = 1 to n - 1
      for c = 1 to n - 1
        if f[r * n + c] = 1
          move (c - 1) * f (r - 1) * f
          rect f * 0.9 f * 0.9
        .
      .
    .
    sleep 0.05
  .
.
subr patch
  f[n + 1] = 1
  f[n + n - 1] = 1
  f[n * (n - 1) + 1] = 1
  f[n * n - 1] = 1
.
subr update
  swap f[] p[]
  for r = 1 to n - 1
    sm = 0
    i = r * n
    sr = p[i - n + 1] + p[i + 1] + p[i + n + 1]
    for c = 1 to n - 1
      i += 1
      sl = sm
      sm = sr
      sr = p[i - n + 1] + p[i + 1] + p[i + n + 1]
      s = sl + sm + sr
      if s = 3 or s = 4 and p[i] = 1
        f[i] = 1
      else
        f[i] = 0
      .
    .
  .
.
for part = 1 to 2
  f[] = [ ]
  for i range n
    f[] &= 0
  .
  for s$ in inp$[]
    f[] &= 0
    for i range n - 1
      f[] &= if substr s$ i 1 = "#"
    .
  .
  for i range n + 1
    f[] &= 0
  .
  if part = 2
    call patch
  .
  call show
  len p[] len f[]
  for step range 100
    call update
    if part = 2
      call patch
    .
    call show
  .
  sum = 0
  for v in f[]
    sum += v
  .
  print sum
.
