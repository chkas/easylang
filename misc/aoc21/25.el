# AoC-21 - Day 25: Sea Cucumber
#
# Updating happens in-place. Therefore you
# have to notice if a cucumber has moved
# in order not to move it again.
#
visualization = 1
# 
repeat
  s$ = input
  until s$ = ""
  nc = len s$
  for c$ in strchars s$
    if c$ = ">"
      m[] &= 1
    elif c$ = "v"
      m[] &= 2
    else
      m[] &= 0
    .
  .
  nr += 1
.
# 
background 001
col[] = [ 0 030 060 ]
func show . .
  if visualization = 0
    break 1
  .
  clear
  sc = 100 / nc
  sc1 = sc * 1.1
  for i range len m[]
    if m[i] >= 1
      color col[m[i]]
      x = i mod nc
      y = i div nc
      move sc * x sc * y
      rect sc1 sc1
    .
  .
  sleep 0.01
.
func step . done .
  done = 1
  for i range len m[]
    if i mod nc = 0
      v0 = m[i]
      moved = 0
    .
    if moved = 1
      moved = 0
    elif m[i] = 1
      in = i + 1
      if in mod nc = 0
        in -= nc
        h = v0
      else
        h = m[in]
      .
      if h = 0
        m[in] = 1
        m[i] = 0
        moved = 1
        done = 0
      .
    .
  .
  for c range nc
    i = c
    v0 = m[i]
    moved = 0
    for r range nr
      if moved = 1
        moved = 0
      elif m[i] = 2
        in = i + nc
        if r = nr - 1
          in = c
          h = v0
        else
          h = m[in]
        .
        if h = 0
          m[in] = 2
          m[i] = 0
          moved = 1
          done = 0
        .
      .
      i += nc
    .
  .
.
# 
for step range 9999
  call step done
  call show
  if done = 1
    print step + 1
    break 1
  .
.
